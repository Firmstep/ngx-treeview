{
  "name": "angular-ngx-treeview",
  "version": "0.0.0",
  "license": "MIT",
  "description": "An Angular treeview component with checkbox",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "test-coverage": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "prebuild": "rimraf tmp dist",
    "preinstall": "./install.sh",
    "build": "gulp inline-templates && ngc -p tsconfig-lib.json && copyfiles package.json README.md LICENSE dist",
    "postbuild": "rimraf tmp",
    "pub": "npm run build && npm publish dist",
    "predeploy": "rimraf dist",
    "deploy": "ng build --prod --base-href /ngx-treeview/ && node gh-pages.js"
  },
  "private": true,
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "ngx-treeview",
    "tree",
    "treeview",
    "tree-view",
    "checkbox",
    "dropdown",
    "recursive",
    "angular tree component"
  ],
  "author": "Leo Vo <leovo2708@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Firmstep/ngx-treeview.git"
  },
  "bugs": {
    "url": "https://github.com/Firmstep/ngx-treeview/issues"
  },
  "dependencies": {
    "@angular/animations": "8.0.1",
    "@angular/common": "8.0.1",
    "@angular/compiler": "8.0.1",
    "@angular/core": "8.0.1",
    "@angular/forms": "8.0.1",
    "@angular/platform-browser": "8.0.1",
    "@angular/platform-browser-dynamic": "8.0.1",
    "@angular/router": "8.0.1",
    "bootstrap": "^4.5.0",
    "lodash": "^4.17.15",
    "tslib": "^2.0.0",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.3",
    "@angular-devkit/build-ng-packagr": "0.800.3",
    "@angular/cli": "8.0.3",
    "@angular/compiler-cli": "8.0.1",
    "@angular/language-service": "8.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.12.47",
    "codelyzer": "^6.0.0-next.1",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "3.4.5"
  }
}
